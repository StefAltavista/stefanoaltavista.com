generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Projects {
  id    Int     @id @default(autoincrement())
  title String
  subtitle String
  url String
  gitHub String
  logo String[]
  images String[]
  background String
  color String
  functionality String
  description String
  technologies String
  name  String
}